# ESLint config file
# Rules: http://eslint.org/docs/rules/
# User Guide: http://eslint.org/docs/user-guide/configuring

env:
  node: true

rules:
  indent: [2, "always"]
  dot-notation: 2
  no-new-object: 2
  no-reserved-keys: 2
  no-array-constructor: 2
  no-multi-str: 2
  no-multi-spaces: 2
  no-empty: 0
  no-use-before-define: 0
  no-shadow-restricted-names: 2
  no-unused-vars: 0
  quotes: [2, "single"]
  block-scoped-var: 2
  vars-on-top: 0
  eqeqeq: 2
  use-isnan: 2
  no-eq-null: 2
  brace-style: 2
  spaced-line-comment: 2
  valid-jsdoc: 2
  space-infix-ops: 2
  eol-last: 2
  padded-blocks: [2, "never"]
  space-before-blocks: 2
  comma-dangle: [2, "never"]
  comma-style: 2
  semi: [2, always]
  semi-spacing: 2
  no-extra-boolean-cast: 2
  no-spaced-func: 2
  no-wrap-func: 2
  one-var: [2, "never"]
  camelcase: 2
  new-cap: 2
  consistent-this: [1, "_this"]
  func-names: 0
  key-spacing: 2
  no-dupe-keys: 2
  strict: 2
  space-before-function-paren: [2, "never"]
